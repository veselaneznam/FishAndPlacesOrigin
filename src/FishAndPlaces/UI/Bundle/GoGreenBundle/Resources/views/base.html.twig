<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <link rel="icon" type="image/x-icon" href="{{ asset('favico n.ico') }}"/>
    <link rel="stylesheet" href="{{ asset('css/jquery-ui.css') }}"/>
    <link rel="stylesheet" href="{{ asset('css/jquery-ui.structure.css') }}"/>
    <link rel="stylesheet" href="{{ asset('css/jquery-ui.theme.css') }}"/>
    <link rel="stylesheet" href="{{ asset('css/grid.css') }}"/>
    <link rel="stylesheet" href="{{ asset('bootstrap/css/bootstrap.min.css') }}"/>
    <link rel="stylesheet" href="{{ asset('css/custom.css') }}"/>
    <title>{% trans %}Go Green{% endtrans %}</title>
</head>
<body>
<nav class="navbar navbar-inverse">
    <div class="container-fluid">
        <div class="header-left" style="float: left; width: 40%">&nbsp;</div>
        <div class="navbar-header" style="width: 20%">
            <a class="navbar-brand" href="{{ path('dam') }}">{% trans %}Go Green{% endtrans %}</a>
        </div>
        <div class="header-right" style="float: right; width: 40%">&nbsp;</div>
    </div>
</nav>
    {% block body %}
        {% for flashMessage in app.session.flashbag.get('error') %}
            <div class="alert alert-danger">
                {{ flashMessage }}
            </div>
        {% endfor %}
        {% for flashMessage in app.session.flashbag.get('notice') %}
            <div class="alert alert-info">
                {{ flashMessage }}
            </div>
        {% endfor %}
    <div class="jumbotron">
        <div class="container text-center center-block">
        {% include '@GoGreen/dam/search.html.twig' %}
            {% block content %}
            {% endblock %}
        </div>
    </div>
    {% endblock %}
        <footer class="container-fluid">
            {% include '@GoGreen/core/_contact_form.html.twig' %}
            <p>CopyRight &copy; {{ date() | date('Y') }} НаЗелено.com</p>
        </footer>
{% block javascripts %}
    <script src="{{ asset('js/jquery.js') }}"></script>
    <script src="//ajax.googleapis.com/ajax/libs/angularjs/1.2.0/angular.min.js"></script>
    <script src="{{ asset('js/jquery-ui.js') }}"></script>
    <script src="{{ asset('js/main.js') }}"></script>
    <script src="{{ asset('bootstrap/js/bootstrap.min.js') }}"></script>
    <script src="{{ asset('js/custom.js') }}"></script>
    <script src="{{ asset('js/user_location.js') }}"></script>
    <script src="{{ asset('js/contact-us.js') }}"></script>
    <!-- Go to www.addthis.com/dashboard to customize your tools --> <script type="text/javascript" src="//s7.addthis.com/js/300/addthis_widget.js#pubid=ra-58ed0a8755491266"></script>
    <script id="dsq-count-scr" src="//ribar-bg.disqus.com/count.js" async></script>
{% endblock %}
</body>
</html>
