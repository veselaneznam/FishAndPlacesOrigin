        <div class="col-ls-2 text-justify">
            <h5>{{ name }}</h5>
            <table class="table table-condensed">
                <thead>
                <tr colspan="2">
                    <th>

                        {% if mainImage != null %}
                            <img src="{{ mainImage }}" width="200px" title="{{ name }}" >
                        {% else %}
                            <img src="{{ asset('uploads/images/4b009f43fe1cb51540022ad3fdcca0e7.jpeg') }}"  width="200px" class="img-responsive">
                        {% endif %}
                    </th>

                </tr>
                <tr> <th><h5>{% trans %}Info{% endtrans %}</h5></th></tr>
                </thead>
                <tbody>
                <tr>
                    <td style="color: green; font-weight: bold;">{% trans %}Rating{% endtrans %}</td> <td>{{ rating }}</td>
                </tr>
                <tr>
                    <td style="color: green; font-weight: bold;">{% trans %}Price{% endtrans %}</td> <td>{{ priceProPerson }}</td>
                </tr>
                <tr>
                    <td style="color: green; font-weight: bold;">{% trans %}Address{% endtrans %}</td> <td>{{ address }}</td>
                </tr>
                <tr>
                    <td><a  class="btn btn-success" href="{{ path('green_object_map_direction', {'id': id}) }}">{% trans %}Directions{% endtrans %}</a></td>
                    <td><a class="btn btn-info" href="{{ path('green_object_view', {'id': id}) }}">{% trans %}More details{% endtrans %}</a></td>
                </tr>
                </tbody>
            </table>
        </div>
